{
  "name": "rollupjs.org",
  "author": "Rich Harris",
  "version": "0.1.0",
  "devDependencies": {
    "clean-css": "^4.1.3",
    "console-group": "^0.3.3",
    "eslint": "^4.0.0",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.3.0",
    "highlight.js": "^9.12.0",
    "locate-character": "^2.0.1",
    "marked": "^0.3.6",
    "now": "^7.0.2",
    "rollup": "^0.43.0",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-svelte": "^2.0.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0",
    "sander": "^0.6.0",
    "surge": "^0.19.0",
    "svelte": "^1.22.2"
  },
  "scripts": {
    "start": "node server/dist/bundle.js",
    "lint": "node node_modules/eslint/bin/eslint src --ext .js --ext .html",
    "update_overview": "node scripts/update_overview",
    "rollup": "rollup -c",
    "rollup:dev": "export DEV=true && rollup -c -w",
    "dev": "node scripts/prep && npm run rollup:dev & npm start",
    "build": "node scripts/prep && npm run rollup",
    "stage": "now",
    "deploy": "now && now alias",
    "deploy:ci": "now --token $NOW_TOKEN && now alias --token $NOW_TOKEN"
  },
  "dependencies": {
    "acorn": "^5.0.3",
    "codemirror": "^5.26.0",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "locate-character": "^2.0.1",
    "roadtrip": "^0.5.1"
  },
  "now": {
    "alias": "rollupjs.org",
    "files": [
      "examples",
      "guide",
      "scripts",
      "universal",
      "public",
      "server",
      "service-worker",
      "client",
      "rollup.config.js"
    ]
  }
}
